{
	"extends": ["react-app", "eslint:recommended"],
	"plugins": ["prettier"],
	"rules": {
		"prettier/prettier": "error"
	},
	"globals": {
    "React": true,
    "google": true,
    "mount": true,
    "mountWithRouter": true,
    "shallow": true,
    "shallowWithRouter": true,
    "context": true,
    "expect": true,
    "jsdom": true,
    "JSX": true
	}
}
